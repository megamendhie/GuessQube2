<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

<item android:state_pressed="true">
	<shape android:shape="rectangle">
	<gradient android:startColor="#D4F0FB" 
	       android:endColor="#939798"
	       android:angle="225" />
	<corners android:radius="12dp"/>
	<stroke android:width="5dp" android:color="#E46018" />
	</shape>
</item>

<item>
	<shape android:shape="rectangle">
	     	<gradient android:startColor="#06F0FB" 
	       android:endColor="#01179A"
	       android:angle="270" />
	<corners android:radius="16dp"/>
	<stroke android:width="3dp" android:color="#A6B4E8" />
	</shape>
</item>

</selector>